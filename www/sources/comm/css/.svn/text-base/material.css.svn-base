.materialhead {}

.materialhead .row,
.materialhead .col {
	margin: 0px;
	padding: 0px;
}

.materialhead .twotab {
	position: absolute;
	top: 44px;
	margin-left: -5px;
}

.materialhead .twotab span {
	display: block;
	margin-top: -0.9rem;
	font-size: 12.5px;
	font-weight: 700;
}

.materialhead .twotab a {
	border-radius: 0px;
	height: 57px;
}

.materialhead .twotab .noselected {
	background-color: #f6f6f6;
	border-color: #ddd;
	color: #333;
}

.materialhead .twotab .selected {
	background-color: #38c;
	border-color: #38c;
	color: #ffffff;
}

.serchbtn {
	margin-top: 57px;
}

.marterialitem,
.marterialitem .row,
.marterialitem .col {
	margin: 0px;
	padding: 0px;
}

.marterialitem img {
	width: 5rem;
	margin-bottom: 1rem;
}

.marterialitem .detail {
	padding: 0.8rem 1rem 0px 1.5rem;
	border-right: solid 1px #ddd;
	color: #333;
}

.marterialitem .detail h2 {
	margin: .45em 0;
	font-weight: 700;
}

.marterialitem .detail p {
	font-size: 0.75rem;
	font-weight: 400;
	margin: .6em 0;
	color: #333;
}

.col-70 {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 70%;
	-moz-box-flex: 0;
	-moz-flex: 0 0 70%;
	-ms-flex: 0 0 70%;
	flex: 0 0 70%;
	max-width: 70%;
}

.marterialitem a {
	margin-top: 2rem;
}

.downloadmaterial {
	text-align: center;
}
.downloadmaterial .content{
	
	padding: 16px;
}
.downloadmaterial .downdetail {
	width: 80%;
	margin: auto;
	font-size: 1rem;
	color: #333;
}
.downloadmaterial button{
	    min-height: 2rem;
    height: 2rem;
    line-height: 2rem;
        margin-top: 0.6rem;
}
.downloadmaterial .process{
	margin-top: 50px;
	background-color: #D9D9D9;
	text-align: center;
	border-radius: 0.5rem;
}
.downloadmaterial .rate{
	display: block;
	position: absolute;
	width: 100%;
}
.downloadmaterial .processbar{
	width: 0%;
	height:1rem;
	color:#5cb85c; 
	background-color: #5cb85c;
	border-radius: 0.5rem;
}
